{
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vsliveshare.vsliveshare",
        "ritwickdey.liveserver",
        "vsls-contrib.gitdoc"  // Ensure GitDoc is listed here for automatic installation
      ],
      "settings": {
        "gitdoc.enabled": true,
        "git.autofetch": true,
        "gitdoc.autoCommitDelay": 5000
      }
    }
  },
}